{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/registro.html",
      "dest": "/registro.html"
    },
    {
      "src": "/chat/(.*)",
      "dest": "/chat/$1"
    },
    {
      "src": "/wp-content/(.*)",
      "dest": "/wp-content/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {},
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/chat",
      "destination": "/chat/index.html"
    },
    {
      "source": "/chat/",
      "destination": "/chat/index.html"
    },
    {
      "source": "/registro",
      "destination": "/registro.html"
    },
    {
      "source": "/bem-vindo",
      "destination": "/bem-vindo.html"
    },
    {
      "source": "/inicial",
      "destination": "/inicial.html"
    },
    {
      "source": "/pagamento-aprovado",
      "destination": "/pagamento-aprovado.html"
    },
    {
      "source": "/editar-perfil",
      "destination": "/editar-perfil.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/wp-content/uploads/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "trailingSlash": true,
  "cleanUrls": false
}
